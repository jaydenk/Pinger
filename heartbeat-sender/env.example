# URL of the pinger instance on chipotle (no trailing slash)
HEARTBEAT_URL=https://pinger.yourdomain.com

# Must match HEARTBEAT_API_KEY on the pinger server
HEARTBEAT_API_KEY=your_shared_secret_here

# Heartbeat ID - must match the address in the pinger CSV
HEARTBEAT_ID=wan-home

# Seconds between heartbeats
HEARTBEAT_INTERVAL=30
